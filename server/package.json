{
  "name": "krs_viewer_app",
  "version": "1.0.0",
  "description": "Web application for viewing company financial data based on KRS yearly reports in .xml format.",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server/src/app.js",
    "dev": "nodemon src/app.ts",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx ts-node src/drizzle/migrate.ts",
    "db:load": "npx ts-node sample_data/load_data.ts"
  },
  "author": "Kamil Safaryjski",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/xml2js": "^0.4.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.0",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "globals": "^15.6.0",
    "nodemon": "^3.1.3",
    "prettier": "3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1"
  }
}
